 run = "go build -o sector-analyzer . && ./sector-analyzer"              
  modules = ["go-1.21"]                                          
                                                                     
  [deployment]                                                       
  run = ["sh", "-c", "go build -o sector-analyzer . && ./sector-analyzer"]
  deploymentTarget = "cloudrun"                                      
                                                                     
  [deployment.healthCheck]                                                
  path = "/health"

  [[ports]]
  localPort = 8000
  externalPort = 80

[agent]
expertMode = true
